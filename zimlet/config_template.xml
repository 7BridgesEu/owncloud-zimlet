<zimletConfig name="tk_barrydegraaff_owncloud_zimlet" version="1.0">
    <global>
        <property name="disable_password_storing">false</property>
        <property name="owncloud_zimlet_server_name"></property>
        <property name="owncloud_zimlet_server_port"></property>
        <property name="owncloud_zimlet_server_path">/owncloud/remote.php/webdav/</property>
        <property name="owncloud_zimlet_oc_folder">/owncloud</property>
        <property name="owncloud_zimlet_default_folder"></property>
        <property name="owncloud_zimlet_ask_folder_each_time">false</property>
        <property name="owncloud_zimlet_disable_rename_delete_new_folder">false</property>
        <property name="owncloud_zimlet_extra_toolbar_button_title">Open ownCloud tab</property>
        <property name="owncloud_zimlet_extra_toolbar_button_url">https://owncloud.com</property>
    </global>
</zimletConfig>
